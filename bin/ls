#!/usr/bin/env node

const chalk = require('chalk')
const templateList = require('../template_list')

/**
 * Padding.
 */

console.log()
process.on('exit', () => {
  console.log()
})

console.log(' templates list:')
console.log()
templateList.forEach(repo => {
  console.log(
    '  ' + chalk.green('~') +
    '  ' + chalk.blue(repo.name) +
    ' - ' + repo.description)
})
